import java.util.*;
public class Main
{
	public static void main (String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int  sum=0;
		System.out.println("Enter the number of digits");
		int n = sc.nextInt();
		int [] arr = new int [n];
		System.out.println("Enter the digits");
		for(int i = 0; i<n; i++) {
			arr[i] = sc.nextInt();
			sum+=arr[i];
			}
			System.out.println("The average of the the array is "+sum/n);
	}
}